<?xml version="1.0" encoding="utf-8"?>


<configuration>

  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,log4net" />
  </configSections>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2"/>
  </startup>

  <appSettings>
    <add key="ThreadCount" value="2" />
    <add key="PrinterIP" value="10.184.129.248" />
    <add key="PrintIntervalTime" value="90" />

    <!--application will use loop model, the application will execute with loop number-->
    <add key="Loop" value="false" />
    <add key="LoopCount" value="20" />
    <!--delay time Minutes-->
    <add key="LoopDelay" value="30" />
    
    <!--application execute model: Time,Count,Manual-->
    <add key="Model" value="Count" />
    <!--application execute durtion time minute-->
    <add key="ExecuteTime" value="1500" />
    <!--application execute Count this value must more or equal the threadCount-->
    <add key="ExecuteCount" value="2" />
  </appSettings>
  
  
  <log4net xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" debug="true">
    <appender name="RollingLogFileAppender" type="log4net.Appender.RollingFileAppender">
      <file value="..\\Log\\Service.log" />
      <!--file可以指定具体的路径 eg : d:\\test.log。不指定的话log被生成在项目的bin/Debug 或者 bin/Release目录下 （web的项目 默认生成在根目录下）-->
      <lockingModel type ="log4Net.Appender.FileAppender+MinimalLock"/>
      <datapattern value ="(yyyymmdd)"/>
      <appendToFile value="true" />
      <rollingStyle value="Size" />
      <maxSizeRollBackups value="10" />
      <!--备份log文件的个数最多10个-->
      <maximumFileSize value="10MB" />
      <!--每个log文件最大是2M，如果超过2M将重新创建一个新的log文件，并将原来的log文件备份。-->
      <staticLogFileName value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <!--指定log的格式-->
        <conversionPattern value="[%date] %-5level:%logger[%M]: %message%newline" />
      </layout>
    </appender>

    <root>
      <level value="INFO" />
      <!--指定将此级别及以上的log打印到log文件中-->
      <appender-ref ref="RollingLogFileAppender" />
    </root>
  </log4net>




</configuration>
