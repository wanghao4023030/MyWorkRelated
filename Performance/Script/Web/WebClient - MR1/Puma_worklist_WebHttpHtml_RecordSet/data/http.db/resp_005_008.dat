.sidebar-nav {
    /*position: absolute;*/
    top: 0;
    width: 275px;
    margin: 0;
    padding: 0;
    list-style: none;
    background-color: #30363E;
}

    .sidebar-nav li {
        /*text-indent: 20px;*/
        line-height: 40px;
    }

        .sidebar-nav li a {
            display: block;
            text-decoration: none;
            color: #fff;
        }

            .sidebar-nav li a:hover {
                text-decoration: none;
                color: #EC802A;
                border-left: 5px solid #e37e05;
                background: rgba(255,255,255,0.2);
            }

            .sidebar-nav li a[disabled]:hover {
                border-left: none;
                color: #fff;
                background: none;
                cursor: not-allowed;
            }

            .sidebar-nav li a:active,
            .sidebar-nav li a:focus {
                text-decoration: none;
            }

            .sidebar-nav li a[disabled] label:hover,
            .sidebar-nav li a[disabled] span:hover {
                cursor: not-allowed;
            }

    .sidebar-nav > .sidebar-brand {
        height: 65px;
        font-size: 18px;
        line-height: 60px;
    }

        .sidebar-nav > .sidebar-brand a {
            color: #fff;
        }

            .sidebar-nav > .sidebar-brand a:hover {
                color: #EC802A;
                background: none;
            }
    .sidebar-padding-left {
        padding-left: 20px;
    }
    .sidebar-icon {
        color: #fff;
        display: none;
    }

    .sidebar-nav li:hover > .sidebar-icon{
        display: inline;
    }

