.round_button {
    border: 1px solid #dfdfdf;
    position: relative;
    display: inline-block;
    box-sizing: content-box;
    overflow: visible;
    padding: 0;
    margin: 0;
    border-radius: 20px;
    cursor: pointer;
    box-shadow: #dfdfdf 0 0 0 0 inset;
    transition: .3s ease-out all;
    -webkit-transition: .3s ease-out all;
    text-align: center;
    line-height: 30px;
    font-size: 12px;
    background-color: white;
}

.round_button[disabled],
.round_button[disabled] span {
    background-color: #dfdfdf;
    color: #aeadad !important;
    cursor: not-allowed !important;
}

.round_button span {
    background-color: white;
    color: black;
}

.print_button {
    font-size: 14px;
    text-align: center;
    line-height: 30px;
}

    .print_button:hover {
        color: #09a8f8;
    }

.reprint_text {
    font-size: 13px;
    text-align: center;
    line-height: 30px;
    padding: 3px;
}

.partial_print {
    position: relative;
    display: table;
    margin: -15px auto auto auto;
    border-collapse: separate;
    text-align: center;
    line-height: 30px;
    font-size: 12px;
    top: 50%;
}

.partial_print_left {
    display: table-cell;
    width: 50%;
    border: 1px solid #dfdfdf;
    position: relative;
    box-sizing: content-box;
    overflow: visible;
    padding: 0;
    margin: 0;
    border-radius: 20px 0 0 20px;
    cursor: pointer;
    box-shadow: #dfdfdf 0 0 0 0 inset;
    transition: .3s ease-out all;
    -webkit-transition: .3s ease-out all;
    text-align: center;
    line-height: 30px;
    font-size: 12px;
}

.partial_print_right {
    display: table-cell;
    width: 50%;
    border: 1px solid #dfdfdf;
    position: relative;
    box-sizing: content-box;
    overflow: visible;
    padding: 0;
    margin: 0;
    border-radius: 0 20px 20px 0;
    cursor: pointer;
    box-shadow: #dfdfdf 0 0 0 0 inset;
    transition: .3s ease-out all;
    -webkit-transition: .3s ease-out all;
    text-align: center;
    line-height: 30px;
    font-size: 12px;
}

